
include color

bakgrunn= overlay(rectangle(220, 40, "solid", "white"),
    rectangle(220, 160, "solid", "red"))


vertikal= overlay(rectangle(20, 160, "solid", "Blue"), 
  overlay(rectangle(320, 20, "solid", "blue"),
    rectangle(40, 160, "solid", "white")))

vert= put-image(vertikal, 80, 80, bakgrunn)	

vert
